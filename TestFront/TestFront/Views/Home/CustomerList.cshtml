@using Microsoft.AspNetCore.Mvc.Rendering
@using TestFront.HttpHelpers
@model List<TestFront.HttpHelpers.Customer>

@{ 
    if(Model.Count == 0){
        <p>No customers retrieved</p>
    }
    else
    {
        foreach(Customer customer in Model)
        {
            <p>@customer.Name</p>
        }
    }


}